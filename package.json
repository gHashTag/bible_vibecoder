{
  "name": "bible_vibecoder",
  "version": "1.0.0",
  "description": "Vibecoding Telegram Bot",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc --project tsconfig.build.json",
    "dev": "bun --watch index.ts",
    "build:bun": "bun build index.ts --outdir ./dist --target bun",
    "build:full": "tsc --project tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "drizzle:generate": "drizzle-kit generate:pg",
    "drizzle:migrate": "bun src/db/migrate.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@apollo/client": "^3.10.8",
    "@types/express": "^5.0.3",
    "@types/js-yaml": "^4.0.9",
    "ai": "^4.3.16",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.2",
    "express": "^5.1.0",
    "graphql": "16.11.0",
    "inngest": "^3.19.1",
    "js-tiktoken": "^1.0.12",
    "js-yaml": "^4.1.0",
    "node-cache": "^5.1.2",
    "node-html-to-image": "^4.0.0",
    "openai": "^4.52.3",
    "pg": "^8.12.0",
    "puppeteer": "^22.12.1",
    "react": "19.1.0",
    "telegraf": "^4.16.3",
    "typescript": "^5.5.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "drizzle-kit": "^0.21.4",
    "inngest-cli": "^0.6.0",
    "vitest": "^1.6.0"
  },
  "module": "src/index.js"
}
