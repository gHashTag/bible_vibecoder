{
  "name": "bible_vibecoder",
  "version": "1.0.0",
  "description": "Vibecoding Telegram Bot",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun src/index.ts",
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "build:full": "tsc --project tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "drizzle:generate": "drizzle-kit generate:pg",
    "drizzle:migrate": "bun src/db/migrate.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.14.1",
    "@langchain/core": "^0.2.16",
    "@langchain/openai": "^0.2.1",
    "@types/js-yaml": "^4.0.9",
    "drizzle-orm": "^0.30.10",
    "inngest": "^3.19.1",
    "js-tiktoken": "^1.0.12",
    "js-yaml": "^4.1.0",
    "node-cache": "^5.1.2",
    "node-html-to-image": "^4.0.0",
    "openai": "^4.52.3",
    "pg": "^8.12.0",
    "puppeteer": "^22.12.1",
    "telegraf": "^4.16.3",
    "telegraf-i18n": "^6.6.0",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5",
    "@apollo/client": "^3.10.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "@types/dotenv": "^8.2.0",
    "drizzle-kit": "^0.21.4",
    "inngest-cli": "^0.6.0",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  },
  "module": "src/index.js"
}
